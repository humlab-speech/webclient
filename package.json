{
  "name": "webclient-ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production --output-path dist",
    "datalab-build": "ng build --configuration=datalab --output-path dist",
    "visp-build": "npm run php:vendor && ng build --configuration=visp --output-path dist",
    "visp-demo-build": "ng build --configuration=visp-demo --output-path dist",
    "visp-pdf-server-build": "ng build --configuration=visp-pdf-server --output-path dist",
    "visp-local-build": "ng build --configuration=visp.dev --output-path dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "devbuild": "ng build --output-path dist --watch",
    "visp-dev": "npm run php:vendor && ng build --configuration=visp.dev --output-path dist --watch",
    "datalab-dev": "ng build --configuration=datalab.dev --output-path dist --watch",
    "php:vendor": "composer install --no-interaction --prefer-dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.11",
    "@angular/cdk": "^18.2.11",
    "@angular/common": "^18.2.11",
    "@angular/compiler": "^18.2.11",
    "@angular/core": "^18.2.11",
    "@angular/forms": "^18.2.11",
    "@angular/material": "^18.2.11",
    "@angular/platform-browser": "^18.2.11",
    "@angular/platform-browser-dynamic": "^18.2.11",
    "@angular/router": "^18.2.11",
    "@popperjs/core": "^2.11.8",
    "@swimlane/ngx-datatable": "^20.1.0",
    "angular-notifier": "^14.0.0",
    "font-awesome": "^4.7.0",
    "js-cookie": "^3.0.1",
    "jszip": "^3.7.1",
    "nanoid": "^5.0.8",
    "ngx-dropzone": "^3.0.0",
    "ngx-popperjs": "^17.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.5.7",
    "shepherd.js": "^14.1.0",
    "speechrecorderng": "^3.0.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.14.28",
    "codelyzer": "^6.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~5.4.0"
  }
}
